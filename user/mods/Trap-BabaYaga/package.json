  "scripts": {
    "zip:files": "node scripts/create-zip.js",
    "prepare:files": "node scripts/prepare-files.js",
    "build:zip": "npm run prepare:files && npm run zip:files",
    "build:release": "npm run rebuild && npm run build:zip",
    "clean": "rimraf -g dist tmp src/**/*.js src/**/*.js.map Trap-BabaYaga*",
    "prettier": "prettier --write .",
